{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-restricted-globals": [
      "error",
      {
        "name": "name",
        "message": "Use local parameter instead."
      }
    ]
  }
}
